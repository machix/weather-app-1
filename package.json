{
  "name": "micro-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for Micro",
  "engines": {
    "npm": ">=5"
  },
  "scripts": {
    "start": "npm-run-all --parallel dev-webpack",
    "build": "build-webpack",
    "dev-webpack": "webpack-dev-server --mode=\"development\" --hot --progress --color --port 3000 --open",
    "prod-webpack": "webpack-dev-server --mode=\"production\" -p --progress --color --port 4000 --open",
    "build-webpack": "webpack --mode=\"production\" -p --progress --colors",
    "eslint": "eslint \"src/**/*.{js,jsx}\"",
    "prettier": "prettier --write \"src/**/*.{js,jsx,css}\""
  },
  "repository": {
    "git": "https://github.com/microlws/weather-app"
  },
